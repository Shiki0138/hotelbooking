{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist", 
  "installCommand": "cd frontend && npm ci --only=production",
  "framework": null,
  "env": {
    "SKIP_TYPE_CHECK": "true",
    "NODE_ENV": "production"
  },
  "functions": {
    "maxDuration": 30
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}